

@using Web_cafe_film.Models;
@model List<Account>

<thead>
    <tr>
        <th>Mã Nhân Viên</th>
        <th>Số thứ tự nhân viên</th>
        <th>Tên Tài Khoản</th>
        <th>Mật Khẩu</th>
        <th>Quyền</th>
        <th>Trạng Thái</th>
    </tr>
</thead>
    @{
        foreach (var item in Model)
        {
            <tbody>
                <tr role="row" class="odd">
                    <td>@item.idAcc</td>
                    <td>@item.idEmp</td>
                    <td>@item.Username</td>
                    <td>@item.Password</td>
                    <td>@item.Permission</td>
                    <td>@item.State</td>
                </tr>
            </tbody>
        }
    }
<tfoot>
    <tr>
        <th>Mã Nhân Viên</th>
        <th>Số thứ tự nhân viên</th>
        <th>Tên Tài Khoản</th>
        <th>Mật Khẩu</th>
        <th>Quyền</th>
        <th>Trạng Thái</th>
    </tr>
</tfoot>















